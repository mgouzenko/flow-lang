proc fill_chan(out int foo){
  1 -> foo;
  2 -> foo;
  3 -> foo;
  poison foo;
}

channel<int> build_chan(){
  channel<int> test;
  fill_chan(test);
  return test;
}

proc printer(in int to_print){
  while(to_print){
    print_int(@to_print);
  }
}

int main(){
  channel<int> test1 = build_chan();
  printer(test1);
  println();
  channel<int> test2 = build_chan();
  printer(test2);

}
