proc numGen(out int ochan){
  list <int> test = [1, 2, 3, 4, 5];

  while(#test > 0) {
    @test -> ochan;
    test = ^test;
  }
  
  poison ochan;
}

proc sum(in int chan) {
  int sum = 0;
  while(chan) {
    sum = sum + @chan;
    print_int(sum);
    println();
  }
}

int main() {
  channel<int> chan;
  numGen(chan);
  sum(chan);
}
