proc numGen(out int ochan){
  list <int> test = [1, 2, 3, 4, 5];
  int i;
  for (i = 0; i < 5; i= i+1)
  {
    @test -> ochan;
    test = ^test;
  }
  poison ochan;
}

proc sum(in int chan) {
  int sum = 0;
  while(chan) {
    sum = sum + @chan;
  }
  print_int(sum);
}

int main() {
  channel<int> chan;
  numGen(chan);
  sum(chan);
}